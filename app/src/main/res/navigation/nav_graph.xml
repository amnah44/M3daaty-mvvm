<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.graps.m3daaty.ui.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home"/>
    <fragment
        android:id="@+id/categoryFragment"
        android:name="com.graps.m3daaty.ui.categoris.CategoryFragment"
        android:label="CategoryFragment"
        tools:layout="@layout/fragment_category"/>
    <fragment
        android:id="@+id/foodDetailsFragment"
        android:name="com.graps.m3daaty.ui.details.FoodDetailsFragment"
        android:label="FoodDetailsFragment"
        tools:layout="@layout/fragment_food_details">
        <argument
            android:name="recipeData"
            app:argType="com.graps.m3daaty.model.domain.recipeInformation.RecipeInformation" />
    </fragment>

    <fragment
        android:id="@+id/cuisine_recipe"
        android:name="com.graps.m3daaty.ui.mealreslut.MealResultFragment"
        android:label="MealResultFragment"
        tools:layout="@layout/fragment_meal_result"/>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.graps.m3daaty.ui.search.SearchFragment"
        android:label="SearchFragment"
        tools:layout="@layout/fragment_search"/>
    <activity
        android:id="@+id/foodActivity"
        android:name="com.graps.m3daaty.ui.M3daatyActivity"
        android:label="FoodActivity"
        tools:layout="@layout/activity_m3daaty"/>
</navigation>